<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.NothingSuspicious.iftclock"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.READ_CALENDAR"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.NothingSuspicious.iftclock.Menu_Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.NothingSuspicious.iftclock.Rules_List"
            android:label="@string/title_activity_rules__list" >
        </activity>
        <activity
            android:name="com.NothingSuspicious.iftclock.Rule"
            android:label="@string/title_activity_rule" >
        </activity>
    </application>

</manifest>
